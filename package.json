{
  "productName": "Phorg",
  "name": "phorg",
  "version": "1.0.0",
  "description": "Cross-platform app to bulk rename photos based on the date they were created in the EXIF data.",
  "main": "app.js",
  "scripts": {
    "build-dev": "node ./node_modules/webpack-cli/bin/cli.js --mode development --config webpack.config.js",
    "build-prod": "node ./node_modules/webpack-cli/bin/cli.js --mode production --config webpack.config.js",
    "dev": "NODE_ENV=development && npm run build-dev && electron .",
    "prod": "npm run build-prod && electron .",
    "package": "TODO -- build-prod then package electron"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenlaidlaw/Phorg"
  },
  "dependencies": {
    "exif": "^0.6.0",
    "fs-extra": "^7.0.0",
    "path": "^0.12.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "electron": "^3.0.2",
    "electron-prebuilt-compile": "^1.6.11",
    "electron-react-devtools": "^0.4.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1"
  }
}
